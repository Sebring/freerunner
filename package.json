{
  "name": "freerunner",
  "version": "0.0.6",
  "description": "Freerunner, a typed event driven entity compent system based game engine.",
  "main": "build/freerunner.js",
  "types": "build/freerunner.d.ts",
  "keywords": [
    "game",
    "engine",
    "entity",
    "component",
    "system",
    "ecs",
    "typescript",
    "2d",
    "crafty",
    "craftyjs"
  ],
  "author": "Johan Sebring",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebring/freerunner.git"
  },
  "scripts": {
    "build": "yarn tsc && yarn minify",
    "compile": "tsc",
    "doc": "typedoc --out docs src/freerunner.ts",
    "minify": "esbuild ./src/freerunner.ts --bundle --keep-names --minify --format=esm --outfile=build/freerunner.min.js"
  },
  "dependencies": {},
  "devDependencies": {
    "esbuild": "^0.13.3",
    "standard-version": "^9.3.1",
    "typedoc": "^0.22.3",
    "typescript": "^4.4.2",
    "vite": "^2.6.2"
  }
}
